{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", 4],
    "semi": ["error", "always"],
    "no-console": "off"
  },
  "globals": {
    "process": false,
    "IS_PRODUCTION": false,
    "IS_DEVELOPMENT": false
  }
}